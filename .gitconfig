[core]
	excludesfile = ~/.gitignore
[color]
	ui = true
[alias]
	s = status -sb
	st = status -sb
	sta = status -sb
	stat = status -sb
	statu = status -sb
	co = checkout
	ci = commit
	br = branch
	graph = log --oneline --color --graph --decorate
[push]
	default = simple
[include]
	path = .local.gitconfig
