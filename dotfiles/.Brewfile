tap 'homebrew/core'
tap 'homebrew/bundle'

# Core
brew 'stow'
brew 'bash'
brew 'coreutils'
brew 'git'
brew 'git-lfs'
brew 'tig'
brew 'tmux'
brew 'vim'
brew 'reattach-to-user-namespace'

# Ruby
brew 'ruby'
brew 'chruby'
brew 'ruby-install'

# Elixir
brew 'elixir'

# Python
brew 'python'
